<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/themes/base/css", "~/Content/css", "~/bundles/bootstrapCSS")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery", "~/bundles/KoMVC", "~/bundles/bootstrapJS")
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/Scripts/Layout/menu.js")
</head>
<body>
    <form id="form1" runat="server">
    <div class="container">
        <div class="container-fluid">
            <div class="row-fluid">
                <div class="span12 Menu navbar-fixed-top">
                    <div class="nav-wrap">
                        <ul class="group" id="example-two">
                            <li class="current_page_item_two"><a href="#">Home</a></li>
                            <li><a href="#">My Profile</a></li>
                            <li><a href="#">My Projects</a></li>
                            <li><a href="#">Tasks <strong style="color: White">+1</strong></a></li>
                            <li><a href="#">Reports</a></li>
                            <li><a href="#">Messages <strong style="color: White">+5</strong></a></li>
                            <li><a href="#">Reviews <strong style="color: White">+1</strong></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12 Header">
                    <h1>
                        Next Project</h1>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12 contend">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    </form>
    
</body>
</html>